{
    "name": "timothydc/laravel-stubs",
    "description": "Useful stubs for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Timothy De Cort",
            "email": "mail@timothydc.be",
            "homepage": "https://www.timothydc.be"
        }
    ],
    "homepage": "https://github.com/timothydc/laravel-stubs",
    "keywords": ["timothydc"],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "TimothyDC\\LaravelStubs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TimothyDC\\LaravelStubs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TimothyDC\\LaravelStubs\\LaravelStubsServiceProvider"
            ],
            "aliases": {
                "LaravelStubs": "TimothyDC\\LaravelStubs\\Facades\\LaravelStubs"
            }
        }
    }
}
